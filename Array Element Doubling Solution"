#include <iostream>   // dubble array value
using namespace std;
int main () {
    int a;
    cout<<"enter the value :";
    cin>>a;
    int array[a];
    for(int i=0;i<a;i++){
           cout<<"enter the element of array :";
           cin>>array[i];
    }

    for( int i =0; i<a; i++) 
    { 
        cout<<array[i]*2<<endl;
    }
    

    return 0;
}
