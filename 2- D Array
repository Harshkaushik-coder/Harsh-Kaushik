3include <iostream>
using namespace std;
int main() {
int n,m;
cout<<"Enter number of Rows ansd Columns :";
cin>>n>>m;
int arr[n][m];
for(int i=0;i<n;i++){
for(int j=0;i<m;j++){
cin>>arr[i][j];
}
}
cout<<"matrix is :\n";
for(int i=0; i<n;i++){
for(int j=0; j<m;j++){
cout<<arr[i][j]<<" ";
}
cout<<endl;
}

return 0;
}
